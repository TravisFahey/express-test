{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Launch Express Server",
            "type": "node",
            "request": "launch",
            "program": "${workspaceFolder}/bin/www",
            "restart": true,
            "env": {
                "NODE_ENV": "development"
            },
            "console": "integratedTerminal"
        },
        {
            "name": "Attach to Running Server",
            "type": "node",
            "request": "attach",
            "port": 9229
        }
    ]
}
